AAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;AACA,OALA;AAMA,KATA;AAUA,IAAA,QAVA,oBAUA,CAVA,EAUA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,QAAA,CAAA,UAAA,KAAA,EAAA;AACA,YAAA,KAAA,EAAA;AACA,cAAA,GAAA,GAAA,WAAA;AACA,cAAA,MAAA,GAAA,MAAA;AACA,cAAA,IAAA,GAAA,MAAA,CAAA,QAAA;AACA,cAAA,OAAA,GAAA;AACA,4BAAA;AADA,WAAA;;AAGA,cAAA,MAAA,CAAA,UAAA,EAAA;AACA,YAAA,MAAA,GAAA,KAAA;AACA,YAAA,GAAA,GAAA,eAAA,MAAA,CAAA,UAAA;AACA;;AACA,UAAA,OAAA,CAAA;AACA,YAAA,GAAA,EAAA,GADA;AAEA,YAAA,MAAA,EAAA,MAFA;AAGA,YAAA,IAAA,EAAA,IAHA;AAIA,YAAA,OAAA,EAAA;AAJA,WAAA,CAAA,CAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,aAAA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,gBAAA;AACA,WARA,EAQA,KARA,CAQA,UAAA,GAAA,EAAA;AACA,gBAAA,GAAA,EAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,cAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,KAAA;AACA;AACA,WAbA;AAcA,UAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,SA1BA,MA0BA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gBAAA;AACA,iBAAA,KAAA;AACA;AACA,OA/BA;AAgCA,KA5CA;AA6CA,IAAA,SA7CA,uBA6CA;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,WAAA;AACA;AA/CA;AAvBA,CAAA","sourcesContent":["<template>\n  <div>\n    <a-row>\n      <a-col :md=\"12\" :xs=\"24\">\n        <a-breadcrumb style=\"margin: 10px 5px\">\n          <a-breadcrumb-item>\n            <router-link to=\"/token/\">{{ $t('token') }}</router-link>\n          </a-breadcrumb-item>\n          <a-breadcrumb-item>{{ $t('add') }}</a-breadcrumb-item>\n        </a-breadcrumb>\n      </a-col>\n    </a-row>\n\n    <a-card :title=\"$t('fillIn')\">\n      <template>\n        <a-form-model\n          @submit=\"onSubmit\"\n          ref=\"ruleForm\"\n          :model=\"retailer\"\n          :rules=\"rules\"\n          :label-col=\"labelCol\"\n          :wrapper-col=\"wrapperCol\"\n        >\n          <a-row>\n            <a-col :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n              <a-form-model-item :label=\"$t('Nomi')\" prop=\"nomi\">\n                <a-input\n                  :placeholder=\"$t('Nomi')\"\n                  v-model=\"retailer.name\"\n                />\n              </a-form-model-item>\n            </a-col>\n            <a-col :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n              <a-form-model-item :label=\"$t('Qisqacha tarif')\" prop=\"tarif\">\n                <a-input\n                  :placeholder=\"$t('Qisqacha tarif')\"\n                  v-model=\"retailer.description\"\n                />\n              </a-form-model-item>\n            </a-col>\n            <a-col :md=\"12\" :xs=\"24\" style=\"padding: 0 15px\">\n              <a-form-model-item :label=\"$t('Vebsayt')\" prop=\"vebsayt\">\n                <a-input\n                  :placeholder=\"$t('Vebsayt')\"\n                  v-model=\"retailer.website\"\n                />\n              </a-form-model-item>\n            </a-col>\n            <a-col :span=\"24\" style=\"padding: 0 15px\">\n              <a-form-model-item>\n                <a-button type=\"primary\" html-type=\"submit\" @click=\"onSubmit\">\n                  {{ $t('save') }}\n                </a-button>\n                <a-button style=\"margin-left: 10px;\" @click=\"resetForm\">\n                  {{ $t('reset') }}\n                </a-button>\n              </a-form-model-item>\n            </a-col>\n          </a-row>\n        </a-form-model>\n      </template>\n    </a-card>\n  </div>\n</template>\n<script>\nimport request from '@/utils/request'\nimport { mapActions } from 'vuex'\n\nexport default {\n  data () {\n    return {\n      retailerId: this.$route.params.id,\n      labelCol: { span: 24 },\n      wrapperCol: { span: 24 },\n      other: '',\n      loading: false,\n      retailer: {\n        name: '',\n        description: null,\n        website: null\n      },\n      rules: {\n        name: [\n          { required: true, message: this.$t('required'), trigger: 'change' }\n        ]\n      }\n    }\n  },\n  mounted () {\n    if (this.categoryId) this.getCategoryAttrs(this.categoryId)\n  },\n  methods: {\n    ...mapActions(['getCategory', 'getCategories', 'postCategory', 'updateCategory']),\n    getCategoryAttrs (category) {\n      request({\n        url: '/category/' + category,\n        method: 'get'\n      }).then(response => {\n        this.category = response.data\n      })\n    },\n    onSubmit (e) {\n      e.preventDefault()\n      this.$refs.ruleForm.validate(valid => {\n        if (valid) {\n          var url = '/category'\n          var method = 'post'\n          var data = this.category\n          const headers = {\n            'Content-Type': 'application/json'\n          }\n          if (this.categoryId) {\n            method = 'put'\n            url = '/category/' + this.categoryId\n          }\n          request({\n              url: url,\n              method: method,\n              data: data,\n              headers: headers\n          }).then(res => {\n            this.getCategories()\n            this.$router.replace('/category/list')\n          }).catch(err => {\n            if (err) {\n              console.log(err)\n              this.$message.error(err.response.data.error)\n            }\n          })\n          console.log('valid')\n        } else {\n          console.log('error submit!!')\n          return false\n        }\n      })\n    },\n    resetForm () {\n      this.$refs.ruleForm.resetFields()\n    }\n  }\n}\n</script>\n<style>\n  .ant-form-item-control {\n    line-height: normal !important;\n  }\n  @media (max-width: 760px) {\n    .ant-form-item {\n      margin: 0;\n    }\n  .ant-form-item-label {\n    padding-bottom: 4px !important;\n  }\n  }\n</style>\n"],"sourceRoot":"src/custom/Product/Form"}]}